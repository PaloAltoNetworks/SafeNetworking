{
    "mappings": {
      "doc": {
        "dynamic": true,
        "properties": {
          "@version": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "bucket_info": {
            "dynamic": true,
            "properties": {
              "minute_points": {
                "type": "integer"
              },
              "daily_points": {
                "type": "integer"
              },
              "minute_points_remaining": {
                "type": "integer"
              },
              "daily_points_remaining": {
                "type": "integer"
              }
            }
          }
        }
      }
    }
  }